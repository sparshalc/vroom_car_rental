<div class="container">
  <div class="text-center">
    <h4 class="fw-bold text-primary"><%= message %></h4>
    <hr>
    <%= inline_svg_tag('svg/error', width: '200', height: '200', class: 'mt-3 mb-5') %>
    </br>
    <small class="lead text-muted"><%= paragraph %></small>
    <% if booked_by_user %>
      <%= link_to 'View Your Bookings', bookings_path %>
    <% end %>
    <br>
  </div>
</div>
